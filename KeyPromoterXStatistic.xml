<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Strg+Alt+S' or 'Strg+Alt+C'</ShortCut>
          <Description>Settings...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'Strg+Alt+Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Key Promoter X">
        <Item>
          <IdeaActionID>ActivateKeyPromoterXToolWindow</IdeaActionID>
          <Description>Key Promoter X</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>